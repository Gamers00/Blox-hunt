local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("Gamers Hub")

local KillingCheats = PhantomForcesWindow:NewSection("Main")

KillingCheats:CreateToggle("Spam", function(state)
_G.SayMessageRequest = (state and true or false)
wait()
while _G.SayMessageRequest == true do
wait()
local args = {
    [1] = "Gamers Hub the Best!",
    [2] = "All"
}

game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(unpack(args))
end
end)

KillingCheats:CreateButton("esp hiders", function()
players = game:GetService("Players")
while wait(1) do
for i, v in pairs(players:GetChildren()) do
if v.Name ~= players.LocalPlayer.Name then
if v.Character and v.Character:FindFirstChild("Object") then
for j, k in pairs(v.Character.Object:GetChildren()) do
if k:IsA("Part") then
k.BrickColor = BrickColor.new("Hot pink")
end
end
end
end
end
end
end)

KillingCheats:CreateButton("Kill All Hiders", function()
local shock  = game.ReplicatedStorage.GameFunctions.Shock
local refill = game.ReplicatedStorage.GameFunctions.StatChange
 
for _, v in pairs(game.Players:GetPlayers()) do
if v.Character and v.Character:FindFirstChild'Object' then
for i=1, 3 do
shock:FireServer("Hit", v.Character)
for i=1, 2 do
refill:FireServer("Energy", 2)
end
end
end
end
end)

KillingCheats:CreateToggle("Spam Taunt", function(state)
_G.Taunt = (state and true or false)
wait()
while _G.Taunt == true do
wait()
game.ReplicatedStorage.GameFunctions.Taunt:FireServer()
end
end)
local KillingCheats = PhantomForcesWindow:NewSection("Player")

KillingCheats:CreateTextbox("Speed", function(text)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = text
end)
KillingCheats:CreateTextbox("JumpPower", function(text)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = text
end)

local KillingCheats = PhantomForcesWindow:NewSection("Credits")

KillingCheats:CreateButton("Gamers", function()

end)
